{
  "name": "MCP Integration Workflow",
  "version": "1.0.0",
  "steps": [
    {
      "id": "init",
      "type": "swarm_init",
      "params": {
        "topology": "mesh",
        "maxAgents": 3
      }
    },
    {
      "id": "spawn",
      "type": "agent_spawn",
      "params": {
        "type": "researcher",
        "capabilities": [
          "analysis"
        ]
      }
    },
    {
      "id": "task",
      "type": "task_orchestrate",
      "params": {
        "task": "Analyze codebase",
        "strategy": "parallel"
      }
    },
    {
      "id": "results",
      "type": "task_results",
      "params": {
        "format": "detailed"
      }
    }
  ]
}