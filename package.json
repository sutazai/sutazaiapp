{
  "name": "ai-service",
  "version": "1.0.0",
  "scripts": {
    "deploy": "node deploy.js",
    "postdeploy": "npm run health-check",
    "health-check": "node health-check.js",
    "start": "node --enable-source-maps slack-bot.js",
    "test": "jest",
    "test:staged": "jest --findRelatedTests --passWithNoTests --bail",
    "format": "prettier --write .",
    "security": "npm audit",
    "ci": "npm run lint && npm test && npm run security",
    "dependency-check": "npx npm-check-updates",
    "outdated": "npm outdated",
    "train-ai": "node scripts/train_ai.js",
    "test-ai": "jest --config jest.ai.config.js",
    "audit:fix": "npm audit fix --force",
    "security:scan": "npx snyk test",
    "dependency:audit": "npx depcheck",
    "predeploy": "npm audit --production --audit-level=critical",
    "audit": "npm audit --production",
    "lint": "eslint . --ext .js,.ts --max-warnings 0",
    "audit:ai": "python3 -m aisafetycheck --model ./ai_models/production_model.bin",
    "check:deploy": "npm-run-all audit:ai security:scan dependency:audit",
    "test:ci": "jest --ci --coverage",
    "lint-staged": "lint-staged",
    "validate-ai-models": "python3 -m aisafetycheck --model ./ai_models/production_model.bin",
    "test:related": "jest --findRelatedTests",
    "security-check": "npm audit --audit-level=critical",
    "sec-check": "npm audit && npx snyk test"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "dependencies": {
    "@slack/bolt": "4.0.0",
    "dotenv": "16.0.3",
    "@openai/api": "4.2.1",
    "helmet": "^6.0.0",
    "rate-limit": "^2.0.0",
    "ai-engine": "^2.0.0",
    "tensorflow-node": "^3.0.0",
    "@slack/web-client": "^6.0.0",
    "snyk": "^1.0.0",
    "depcheck": "^1.0.0",
    "ai-sdk": "2.3.5",
    "axios": "1.6.8-security.0",
    "swagger-ui-express": "4.6.3",
    "jest": "^29.0.0",
    "@aisafety/validator": "^2.4.0",
    "ai-content-filter": "^1.2.1",
    "lint-staged": "^13.0.0",
    "prettier": "^3.2.5",
    "lodash": "4.17.21-security.0",
    "express": "4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "@types/jest": "^29.0.0",
    "stylelint": "^15.0.0",
    "husky": "^9.0.11",
    "eslint-plugin-security": "^1.7.1"
  },
  "resolutions": {
    "minimist": "1.2.8-security.0",
    "axios": "1.6.8-security.0",
    "lodash": "4.17.21-security.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm audit --production"
    }
  },
  "lint-staged": {
    "*.{js,ts}": ["eslint --fix", "prettier --write --parser typescript"],
    "*.{json,md,yml,yaml}": ["prettier --write"],
    "*.{css,scss}": ["stylelint --fix"]
  }
} 