{
  "system_name": "SutazAI",
  "version": "1.0.0",
  "environment": "production",
  "debug_mode": false,
  
  "directories": {
    "base_dir": "/opt/sutazaiapp/backend",
    "data_dir": "/opt/sutazaiapp/backend/data",
    "models_dir": "/opt/sutazaiapp/backend/data/models",
    "logs_dir": "/opt/sutazaiapp/backend/logs",
    "cache_dir": "/opt/sutazaiapp/backend/data/cache",
    "config_dir": "/opt/sutazaiapp/backend/config",
    "storage_dir": "/opt/sutazaiapp/backend/storage",
    "uploads_dir": "/opt/sutazaiapp/backend/uploads"
  },
  
  "performance": {
    "max_workers": 4,
    "timeout_seconds": 300,
    "max_memory_mb": 8192,
    "enable_gpu": true,
    "gpu_memory_fraction": 0.8,
    "model_cache_size": 5
  },
  
  "features": {
    "enable_neural_processing": true,
    "enable_agent_orchestration": true,
    "enable_knowledge_management": true,
    "enable_web_learning": true,
    "enable_self_modification": false,
    "enable_multi_agent_collaboration": true,
    "enable_real_time_learning": true
  },
  
  "security": {
    "enable_security": true,
    "enable_audit": true,
    "enable_encryption": true,
    "enable_ethical_verification": true,
    "enable_content_filtering": true,
    "enable_access_control": true,
    "audit_level": "full",
    "encryption_algorithm": "AES-256"
  },
  
  "monitoring": {
    "enable_monitoring": true,
    "enable_metrics": true,
    "enable_alerts": true,
    "enable_logging": true,
    "metrics_interval": 60,
    "health_check_interval": 30,
    "log_level": "INFO",
    "log_rotation": true,
    "log_retention_days": 30
  },
  
  "models": {
    "auto_load": true,
    "auto_unload": true,
    "max_loaded_models": 5,
    "model_timeout": 1800,
    "quantization": "auto",
    "optimization_level": "balanced",
    "enable_model_caching": true,
    "cache_compression": true
  },
  
  "agents": {
    "max_agents": 50,
    "agent_timeout": 300,
    "enable_communication": true,
    "enable_collaboration": true,
    "enable_learning": true,
    "memory_retention": true,
    "shared_memory": true,
    "agent_persistence": false
  },
  
  "knowledge": {
    "storage_type": "vector",
    "embedding_model": "sentence-transformers/all-MiniLM-L6-v2",
    "vector_dimensions": 384,
    "max_documents": 100000,
    "enable_search": true,
    "enable_indexing": true,
    "search_similarity_threshold": 0.7,
    "enable_semantic_search": true
  },
  
  "neural_processing": {
    "enable_biological_modeling": true,
    "enable_neuromorphic": true,
    "optimization_level": "balanced",
    "enable_plasticity": true,
    "enable_adaptation": true,
    "learning_rate": 0.001
  },
  
  "web_learning": {
    "enable_web_scraping": true,
    "enable_content_processing": true,
    "enable_knowledge_extraction": true,
    "max_concurrent_requests": 10,
    "request_timeout": 30,
    "respect_robots_txt": true,
    "rate_limit": "1/second"
  },
  
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "cors_origins": ["http://localhost:3000", "http://localhost:8080"],
    "enable_docs": true,
    "enable_auth": true,
    "rate_limit": "100/minute",
    "request_timeout": 300
  },
  
  "database": {
    "type": "sqlite",
    "path": "/opt/sutazaiapp/backend/storage/sutazai.db",
    "enable_wal": true,
    "connection_pool_size": 10,
    "query_timeout": 30
  },
  
  "vector_stores": {
    "primary": "qdrant",
    "qdrant": {
      "host": "localhost",
      "port": 6333,
      "collection_name": "sutazai_vectors",
      "distance_metric": "cosine"
    },
    "chroma": {
      "persist_directory": "/opt/sutazaiapp/backend/data/vectors/chroma",
      "collection_name": "sutazai_docs"
    },
    "faiss": {
      "index_path": "/opt/sutazaiapp/backend/data/vectors/faiss",
      "index_type": "IndexFlatL2",
      "dimension": 384
    }
  },
  
  "ollama": {
    "base_url": "http://127.0.0.1:11434",
    "timeout": 60,
    "max_retries": 3,
    "models": {
      "llama3-8b": {
        "model": "llama3-chatqa",
        "temperature": 0.7,
        "num_ctx": 8192
      },
      "deepseek-coder": {
        "model": "deepseek-coder",
        "temperature": 0.2,
        "num_ctx": 16384
      }
    }
  },
  
  "sandbox": {
    "enable_docker": true,
    "docker_image": "python:3.11-slim",
    "execution_timeout": 30,
    "memory_limit": "512m",
    "cpu_limit": "1.0",
    "network_disabled": true,
    "enable_file_system": false
  }
}