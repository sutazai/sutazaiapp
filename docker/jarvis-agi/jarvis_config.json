{
  "ai_models": {
    "text_model": "ollama",
    "local_llm_model": "tinyllama.2:latest",
    "local_llm_endpoint": "http://ollama:10104",
    "vision_model": "clip-vit-base-patch32", 
    "speech_model": "whisper-base",
    "embedding_model": "sentence-transformers/all-MiniLM-L6-v2",
    "orchestration_model": "tinyllama.2:latest",
    "agi_model": "tinyllama.2:latest",
    "fallback_models": ["tinyllama:latest"]
  },
  "services": {
    "web_port": 8080,
    "api_port": 8081, 
    "websocket_port": 8082,
    "gradio_port": 8083,
    "health_port": 8084
  },
  "features": {
    "voice_enabled": true,
    "vision_enabled": true,
    "web_search": true,
    "file_processing": true,
    "multi_model_orchestration": true,
    "agi_mode": true,
    "real_time_learning": true,
    "multi_language_support": true,
    "code_generation": true,
    "system_integration": true,
    "advanced_reasoning": true,
    "memory_management": true,
    "task_automation": true,
    "creative_capabilities": true
  },
  "integrations": {
    "ollama_endpoint": "http://ollama:10104",
    "ollama_api_key": "local",
    "primary_llm": "local_ollama",
    "openai_api_key": "${OPENAI_API_KEY:-not_required_for_local}",
    "anthropic_api_key": "${ANTHROPIC_API_KEY:-not_required_for_local}",
    "huggingface_token": "${HUGGINGFACE_TOKEN:-not_required_for_local}",
    "google_api_key": "${GOOGLE_API_KEY:-not_required_for_local}",
    "deepl_api_key": "${DEEPL_API_KEY:-not_required_for_local}",
    "chromadb_url": "http://chromadb:8000",
    "qdrant_url": "http://qdrant:6333",
    "postgres_url": "postgresql://sutazai:sutazai_password@postgres:5432/sutazai"
  },
  "performance": {
    "max_concurrent_tasks": 10,
    "memory_limit_gb": 8,
    "cache_size_mb": 512,
    "response_timeout_seconds": 30,
    "batch_processing": true,
    "gpu_acceleration": true
  },
  "security": {
    "rate_limiting": true,
    "input_validation": true,
    "output_filtering": true,
    "secure_storage": true,
    "audit_logging": true
  },
  "capabilities": {
    "dipeshpal_jarvis": {
      "voice_commands": true,
      "basic_nlp": true,
      "task_automation": true,
      "api_integration": true
    },
    "microsoft_jarvis": {
      "multi_model_orchestration": true,
      "task_planning": true,
      "model_selection": true,
      "advanced_reasoning": true
    },
    "danilo_jarvis": {
      "web_interface": true,
      "file_processing": true,
      "real_time_collaboration": true,
      "document_analysis": true
    },
    "sreejan_jarvis_agi": {
      "multimodal_processing": true,
      "agi_capabilities": true,
      "advanced_nlp": true,
      "system_integration": true
    },
    "sutazai_enhancements": {
      "enterprise_security": true,
      "scalable_deployment": true,
      "advanced_monitoring": true,
      "knowledge_management": true,
      "continuous_learning": true
    }
  }
}