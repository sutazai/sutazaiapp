{
  "users": [
    {
      "name": "sutazai",
      "password_hash": "",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "sutazai"
    }
  ],
  "permissions": [
    {
      "user": "sutazai",
      "vhost": "sutazai",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "queues": [
    {
      "name": "ai_agent_tasks",
      "vhost": "sutazai",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length": 10000,
        "x-message-ttl": 86400000
      }
    },
    {
      "name": "system_events",
      "vhost": "sutazai",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length": 5000,
        "x-message-ttl": 43200000
      }
    },
    {
      "name": "resource_updates",
      "vhost": "sutazai",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length": 1000,
        "x-message-ttl": 3600000
      }
    }
  ],
  "exchanges": [
    {
      "name": "sutazai_exchange",
      "vhost": "sutazai",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "sutazai_exchange",
      "vhost": "sutazai",
      "destination": "ai_agent_tasks",
      "destination_type": "queue",
      "routing_key": "agent.task.*",
      "arguments": {}
    },
    {
      "source": "sutazai_exchange",
      "vhost": "sutazai",
      "destination": "system_events",
      "destination_type": "queue",
      "routing_key": "system.*",
      "arguments": {}
    },
    {
      "source": "sutazai_exchange",
      "vhost": "sutazai",
      "destination": "resource_updates",
      "destination_type": "queue",
      "routing_key": "resource.*",
      "arguments": {}
    }
  ]
}