{
  "dashboard": {
    "id": null,
    "uid": "redis-performance",
    "title": "ULTRA Redis Performance Dashboard",
    "tags": ["redis", "cache", "performance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Cache Hit Rate %",
        "targets": [
          {
            "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "thresholds": [
          {"value": 85, "color": "green"},
          {"value": 70, "color": "yellow"},
          {"value": 50, "color": "red"}
        ],
        "alert": {
          "name": "Low Cache Hit Rate",
          "conditions": [
            {
              "evaluator": {"type": "lt", "params": [85]},
              "operator": {"type": "and"},
              "query": {"model": "A", "params": ["A", "5m", "now"]},
              "reducer": {"type": "avg", "params": []},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "message": "Redis cache hit rate dropped below 85%",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "stat",
        "title": "Current Hit Rate",
        "targets": [
          {
            "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
            "legendFormat": "Hit Rate",
            "instant": true,
            "refId": "A"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "red", "value": null},
            {"color": "yellow", "value": 70},
            {"color": "green", "value": 85}
          ]
        },
        "unit": "percent"
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "graph",
        "title": "Operations Per Second",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[1m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Used Memory",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes",
            "legendFormat": "Max Memory",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "bytes", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "graph",
        "title": "Response Time (ms)",
        "targets": [
          {
            "expr": "redis_commands_duration_seconds_total / redis_commands_processed_total * 1000",
            "legendFormat": "Avg Response Time",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ms", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "table",
        "title": "Key Patterns",
        "targets": [
          {
            "expr": "redis_db_keys",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Connected Clients",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients",
            "refId": "A"
          },
          {
            "expr": "redis_blocked_clients",
            "legendFormat": "Blocked Clients",
            "refId": "B"
          }
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "type": "stat",
        "title": "Total Keys",
        "targets": [
          {
            "expr": "sum(redis_db_keys)",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "type": "stat",
        "title": "Evicted Keys",
        "targets": [
          {
            "expr": "redis_evicted_keys_total",
            "instant": true,
            "refId": "A"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 100},
            {"color": "red", "value": 1000}
          ]
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "type": "stat",
        "title": "Memory Fragmentation",
        "targets": [
          {
            "expr": "redis_memory_fragmentation_ratio",
            "instant": true,
            "refId": "A"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 1.5},
            {"color": "red", "value": 2}
          ]
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "type": "stat",
        "title": "Cache Efficiency",
        "targets": [
          {
            "expr": "(redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100) > 85",
            "instant": true,
            "refId": "A"
          }
        ],
        "mappings": [
          {"type": "value", "value": "1", "text": "EXCELLENT"},
          {"type": "value", "value": "0", "text": "NEEDS IMPROVEMENT"}
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "red", "value": null},
            {"color": "green", "value": 1}
          ]
        }
      }
    ]
  }
}