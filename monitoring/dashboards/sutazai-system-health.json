{
  "dashboard": {
    "id": null,
    "uid": "sutazai-system-health",
    "title": "SutazAI System Health Overview",
    "tags": ["sutazai", "health", "overview", "system", "infrastructure"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "annotations": {
      "list": [
        {
          "name": "Alerts",
          "datasource": "Alertmanager",
          "enable": true,
          "iconColor": "red",
          "tags": ["sutazai"]
        },
        {
          "name": "Deployments",
          "datasource": "Loki",
          "enable": true,
          "expr": "{job=\"sutazai-deployment\"} |= \"deployment\"",
          "iconColor": "blue"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "type": "stat",
        "title": "Service Health Status",
        "targets": [
          {
            "expr": "up{job=\"sutazai-backend\"}",
            "legendFormat": "Backend API",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "up{job=\"sutazai-frontend\"}",
            "legendFormat": "Frontend",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "up{job=\"postgres\"}",
            "legendFormat": "PostgreSQL",
            "instant": true,
            "refId": "C"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis",
            "instant": true,
            "refId": "D"
          },
          {
            "expr": "up{job=\"neo4j\"}",
            "legendFormat": "Neo4j",
            "instant": true,
            "refId": "E"
          },
          {
            "expr": "up{job=\"ollama\"}",
            "legendFormat": "Ollama",
            "instant": true,
            "refId": "F"
          },
          {
            "expr": "up{job=\"rabbitmq\"}",
            "legendFormat": "RabbitMQ",
            "instant": true,
            "refId": "G"
          },
          {
            "expr": "up{job=\"qdrant\"}",
            "legendFormat": "Qdrant",
            "instant": true,
            "refId": "H"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "HEALTHY"},
              {"type": "value", "value": "0", "text": "DOWN"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "color": {"mode": "thresholds"}
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "name",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 8},
        "type": "stat",
        "title": "Total Services",
        "targets": [
          {
            "expr": "count(up)",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 8},
        "type": "stat",
        "title": "Healthy Services",
        "targets": [
          {
            "expr": "count(up == 1)",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 8}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 8},
        "type": "stat",
        "title": "Failed Services",
        "targets": [
          {
            "expr": "count(up == 0)",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 8},
        "type": "stat",
        "title": "System Uptime",
        "targets": [
          {
            "expr": "min(time() - process_start_time_seconds{job=\"sutazai-backend\"})",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "dtdurations"
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "type": "timeseries",
        "title": "CPU Usage by Container",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"sutazai.*\"}[5m]) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "type": "timeseries",
        "title": "Memory Usage by Container",
        "targets": [
          {
            "expr": "(container_memory_usage_bytes{name=~\"sutazai.*\"} / container_spec_memory_limit_bytes{name=~\"sutazai.*\"}) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 22},
        "type": "timeseries",
        "title": "Database Connections",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{datname=\"sutazai\"}",
            "legendFormat": "PostgreSQL",
            "refId": "A"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Redis",
            "refId": "B"
          },
          {
            "expr": "neo4j_bolt_connections_opened_total",
            "legendFormat": "Neo4j",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 22},
        "type": "timeseries",
        "title": "Disk Usage",
        "targets": [
          {
            "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"})) * 100",
            "legendFormat": "Root Disk",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=~\"/var/lib/docker.*\"} / node_filesystem_size_bytes{mountpoint=~\"/var/lib/docker.*\"})) * 100",
            "legendFormat": "Docker Volumes",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 22},
        "type": "timeseries",
        "title": "Network Traffic",
        "targets": [
          {
            "expr": "rate(container_network_receive_bytes_total{name=~\"sutazai.*\"}[5m]) * 8",
            "legendFormat": "RX - {{name}}",
            "refId": "A"
          },
          {
            "expr": "rate(container_network_transmit_bytes_total{name=~\"sutazai.*\"}[5m]) * 8",
            "legendFormat": "TX - {{name}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bps",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 6, "w": 4, "x": 0, "y": 30},
        "type": "stat",
        "title": "Active Alerts",
        "targets": [
          {
            "expr": "count(ALERTS{alertstate=\"firing\"})",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 6, "w": 4, "x": 4, "y": 30},
        "type": "stat",
        "title": "Total Requests/sec",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[1m]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 6, "w": 4, "x": 8, "y": 30},
        "type": "stat",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"4..|5..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "gridPos": {"h": 6, "w": 4, "x": 12, "y": 30},
        "type": "stat",
        "title": "Avg Response Time",
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_sum[5m])) / sum(rate(http_request_duration_seconds_count[5m]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 6, "w": 4, "x": 16, "y": 30},
        "type": "stat",
        "title": "AI Models Loaded",
        "targets": [
          {
            "expr": "count(ollama_model_loaded == 1)",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 16,
        "gridPos": {"h": 6, "w": 4, "x": 20, "y": 30},
        "type": "stat",
        "title": "Circuit Breakers Open",
        "targets": [
          {
            "expr": "count(circuit_breaker_state == 1)",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 17,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "type": "timeseries",
        "title": "Container Restart Count",
        "targets": [
          {
            "expr": "increase(container_start_time_seconds{name=~\"sutazai.*\"}[1h])",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 100
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 18,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "type": "logs",
        "title": "System Error Logs",
        "targets": [
          {
            "expr": "{job=~\"sutazai.*\"} |= \"ERROR\" or |= \"FATAL\" or |= \"CRITICAL\" or |= \"error\" or |= \"fatal\" or |= \"critical\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 19,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 44},
        "type": "table",
        "title": "Service Health Details",
        "targets": [
          {
            "expr": "up",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "time() - process_start_time_seconds",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "rate(container_cpu_usage_seconds_total[5m]) * 100",
            "format": "table",
            "instant": true,
            "refId": "C"
          },
          {
            "expr": "(container_memory_usage_bytes / 1024 / 1024)",
            "format": "table",
            "instant": true,
            "refId": "D"
          },
          {
            "expr": "rate(container_network_receive_bytes_total[5m]) + rate(container_network_transmit_bytes_total[5m])",
            "format": "table",
            "instant": true,
            "refId": "E"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {"reducers": ["last"]}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true, "instance": true},
              "renameByName": {
                "job": "Service",
                "Value #A": "Status",
                "Value #B": "Uptime (s)",
                "Value #C": "CPU (%)",
                "Value #D": "Memory (MB)",
                "Value #E": "Network (B/s)"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Status"},
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {"type": "value", "value": "1", "text": "HEALTHY"},
                    {"type": "value", "value": "0", "text": "DOWN"}
                  ]
                },
                {
                  "id": "custom.cellOptions",
                  "value": {
                    "type": "colored-background",
                    "mode": "basic"
                  }
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": null},
                      {"color": "green", "value": 1}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Uptime (s)"},
              "properties": [
                {"id": "unit", "value": "dtdurations"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "CPU (%)"},
              "properties": [
                {"id": "unit", "value": "percent"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Memory (MB)"},
              "properties": [
                {"id": "unit", "value": "decmbytes"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Network (B/s)"},
              "properties": [
                {"id": "unit", "value": "Bps"}
              ]
            }
          ]
        }
      }
    ]
  }
}