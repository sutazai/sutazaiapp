{
  "dashboard": {
    "id": null,
    "uid": "sutazai-api-performance",
    "title": "SutazAI API Performance Dashboard",
    "tags": ["sutazai", "api", "performance", "backend", "endpoints"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "templating": {
      "list": [
        {
          "name": "endpoint",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(http_requests_total{job=\"sutazai-backend\"}, endpoint)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "method",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(http_requests_total{job=\"sutazai-backend\"}, method)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "status_code",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(http_requests_total{job=\"sutazai-backend\"}, status_code)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "type": "stat",
        "title": "API Health",
        "targets": [
          {
            "expr": "up{job=\"sutazai-backend\"}",
            "legendFormat": "Backend API",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "HEALTHY"},
              {"type": "value", "value": "0", "text": "DOWN"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "color": {"mode": "thresholds"}
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "value"
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "type": "stat",
        "title": "Request Rate (RPS)",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", method=~\"$method\"}[1m]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "type": "stat",
        "title": "Error Rate (%)",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"sutazai-backend\", status_code=~\"4..|5..\", endpoint=~\"$endpoint\"}[5m])) / sum(rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) * 100",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "type": "stat",
        "title": "Avg Response Time",
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_sum{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) / sum(rate(http_request_duration_seconds_count{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "type": "timeseries",
        "title": "Response Time Percentiles",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "p99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.90, rate(http_request_duration_seconds_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "p90",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "median",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 3,
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom"}
        },
        "alert": {
          "name": "High API Response Time",
          "frequency": "30s",
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [5]},
              "operator": {"type": "and"},
              "query": {"model": "A", "params": ["A", "5m", "now"]},
              "reducer": {"type": "avg", "params": []},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "message": "API p99 response time exceeded 5 seconds",
          "noDataState": "no_data"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "type": "timeseries",
        "title": "Request Rate by Endpoint",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", method=~\"$method\"}[5m]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20,
              "stacking": {"mode": "none"}
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14},
        "type": "timeseries",
        "title": "HTTP Status Codes",
        "targets": [
          {
            "expr": "sum by (status_code) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", status_code=~\"$status_code\"}[5m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 100,
              "stacking": {"mode": "normal"}
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 14},
        "type": "timeseries",
        "title": "Error Rate by Endpoint",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", status_code=~\"4..|5..\"}[5m])) / sum by (endpoint) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) * 100",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 30
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 14},
        "type": "timeseries",
        "title": "Request Size Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_size_bytes_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_size_bytes_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "median",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "type": "timeseries",
        "title": "Response Size Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_response_size_bytes_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_response_size_bytes_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "median",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_response_size_bytes_sum{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) / sum(rate(http_response_size_bytes_count{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "legendFormat": "average",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "type": "timeseries",
        "title": "Concurrent Connections",
        "targets": [
          {
            "expr": "http_requests_in_flight{job=\"sutazai-backend\"}",
            "legendFormat": "Active Requests",
            "refId": "A"
          },
          {
            "expr": "http_server_connections_total{job=\"sutazai-backend\"}",
            "legendFormat": "Total Connections",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 30},
        "type": "stat",
        "title": "Total Requests (1h)",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[1h]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 4, "w": 4, "x": 4, "y": 30},
        "type": "stat",
        "title": "4xx Errors (1h)",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", status_code=~\"4..\"}[1h]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 30},
        "type": "stat",
        "title": "5xx Errors (1h)",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", status_code=~\"5..\"}[1h]))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 30},
        "type": "stat",
        "title": "Peak RPS (1h)",
        "targets": [
          {
            "expr": "max_over_time(sum(rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[1m]))[1h:1m])",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 16,
        "gridPos": {"h": 4, "w": 4, "x": 16, "y": 30},
        "type": "stat",
        "title": "Slowest Endpoint",
        "targets": [
          {
            "expr": "topk(1, sum by (endpoint) (rate(http_request_duration_seconds_sum{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) / sum by (endpoint) (rate(http_request_duration_seconds_count{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 17,
        "gridPos": {"h": 4, "w": 4, "x": 20, "y": 30},
        "type": "stat",
        "title": "Unique Endpoints",
        "targets": [
          {
            "expr": "count(count by (endpoint) (http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}))",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 18,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 34},
        "type": "heatmap",
        "title": "Response Time Heatmap",
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) by (le)",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
      {
        "id": 19,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 34},
        "type": "logs",
        "title": "API Error Logs",
        "targets": [
          {
            "expr": "{job=\"sutazai-backend\"} |= \"ERROR\" or |= \"error\" or |= \"failed\" or |= \"exception\" | json | status_code >= 400",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true
        }
      },
      {
        "id": 20,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 42},
        "type": "table",
        "title": "Endpoint Performance Summary",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "sum by (endpoint) (rate(http_request_duration_seconds_sum{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) / sum by (endpoint) (rate(http_request_duration_seconds_count{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "sum by (endpoint) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\", status_code=~\"4..|5..\"}[5m])) / sum by (endpoint) (rate(http_requests_total{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m])) * 100",
            "format": "table",
            "instant": true,
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"sutazai-backend\", endpoint=~\"$endpoint\"}[5m]))",
            "format": "table",
            "instant": true,
            "refId": "D"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {"reducers": ["last"]}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true, "job": true, "instance": true},
              "renameByName": {
                "Value #A": "RPS",
                "Value #B": "Avg Response (s)",
                "Value #C": "Error Rate (%)",
                "Value #D": "p95 Response (s)"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Error Rate (%)"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 1},
                      {"color": "red", "value": 5}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Avg Response (s)"},
              "properties": [
                {"id": "unit", "value": "s"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 1},
                      {"color": "red", "value": 5}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "p95 Response (s)"},
              "properties": [
                {"id": "unit", "value": "s"}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "RPS"},
              "properties": [
                {"id": "unit", "value": "reqps"}
              ]
            }
          ]
        }
      }
    ]
  }
}