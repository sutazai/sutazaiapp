{
  "dashboard": {
    "id": null,
    "uid": "sutazai-ollama-comprehensive",
    "title": "SutazAI Ollama Performance - Comprehensive",
    "tags": ["sutazai", "ollama", "ai", "performance", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 2,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Loki",
          "enable": true,
          "expr": "{job=\"sutazai-deployment\"} |= \"deployment\"",
          "iconColor": "rgba(0, 211, 255, 1)",
          "textFormat": "Deployment Event"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "model",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(ollama_model_loaded, model)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "instance",
          "type": "query", 
          "datasource": "Prometheus",
          "query": "label_values(ollama_request_total, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 0},
        "type": "stat",
        "title": "Service Status",
        "targets": [
          {
            "expr": "up{job=\"ollama\"}",
            "legendFormat": "Ollama Service",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "ONLINE"},
              {"type": "value", "value": "0", "text": "OFFLINE"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            },
            "color": {"mode": "thresholds"}
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "value"
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 0},
        "type": "stat",
        "title": "Active Models",
        "targets": [
          {
            "expr": "count(ollama_model_loaded{model=~\"$model\"} == 1)",
            "legendFormat": "Loaded Models",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 0},
        "type": "stat",
        "title": "Current Request Rate",
        "targets": [
          {
            "expr": "sum(rate(ollama_request_total{instance=~\"$instance\"}[1m]))",
            "legendFormat": "Requests/sec",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "type": "timeseries",
        "title": "Response Time Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(ollama_request_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "p99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ollama_request_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.90, rate(ollama_request_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "p90",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ollama_request_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "median",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "pointSize": 3,
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "bottom"}
        },
        "alert": {
          "name": "High Ollama Response Time",
          "frequency": "10s",
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [15]},
              "operator": {"type": "and"},
              "query": {"model": "A", "params": ["A", "5m", "now"]},
              "reducer": {"type": "last", "params": []},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "message": "Ollama p99 response time exceeded 15 seconds",
          "noDataState": "no_data"
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "type": "timeseries",
        "title": "Token Generation Rate",
        "targets": [
          {
            "expr": "rate(ollama_tokens_generated_total{model=~\"$model\"}[5m])",
            "legendFormat": "{{model}} - tokens/sec",
            "refId": "A"
          },
          {
            "expr": "rate(ollama_tokens_processed_total{model=~\"$model\"}[5m])",
            "legendFormat": "{{model}} - processed/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14},
        "type": "timeseries",
        "title": "Success Rate",
        "targets": [
          {
            "expr": "(rate(ollama_request_success_total{instance=~\"$instance\"}[5m]) / rate(ollama_request_total{instance=~\"$instance\"}[5m])) * 100",
            "legendFormat": "Success Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 30
            }
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 14},
        "type": "timeseries",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\".*ollama.*\"}/1024/1024",
            "legendFormat": "Memory Used MB",
            "refId": "A"
          },
          {
            "expr": "container_spec_memory_limit_bytes{name=~\".*ollama.*\"}/1024/1024",
            "legendFormat": "Memory Limit MB",
            "refId": "B"
          },
          {
            "expr": "ollama_model_memory_bytes{model=~\"$model\"}/1024/1024",
            "legendFormat": "{{model}} Memory MB",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 14},
        "type": "timeseries",
        "title": "Concurrent Requests",
        "targets": [
          {
            "expr": "ollama_concurrent_requests{instance=~\"$instance\"}",
            "legendFormat": "Active Requests",
            "refId": "A"
          },
          {
            "expr": "ollama_queued_requests{instance=~\"$instance\"}",
            "legendFormat": "Queued Requests",
            "refId": "B"
          },
          {
            "expr": "ollama_max_concurrent_requests{instance=~\"$instance\"}",
            "legendFormat": "Max Concurrent",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 22},
        "type": "stat",
        "title": "Total Requests (1h)",
        "targets": [
          {
            "expr": "increase(ollama_request_total{instance=~\"$instance\"}[1h])",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 10000}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 22},
        "type": "stat",
        "title": "Error Rate (5m)",
        "targets": [
          {
            "expr": "rate(ollama_request_errors_total{instance=~\"$instance\"}[5m]) * 100",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 22},
        "type": "stat",
        "title": "Avg Generation Time",
        "targets": [
          {
            "expr": "rate(ollama_generation_duration_seconds_sum{model=~\"$model\"}[5m]) / rate(ollama_generation_duration_seconds_count{model=~\"$model\"}[5m])",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 22},
        "type": "stat",
        "title": "Model Load Time",
        "targets": [
          {
            "expr": "ollama_model_load_duration_seconds{model=~\"$model\"}",
            "instant": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "type": "timeseries",
        "title": "Model Usage Distribution",
        "targets": [
          {
            "expr": "rate(ollama_request_total{model=~\"$model\"}[5m])",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 100,
              "stacking": {"mode": "normal"}
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right"}
        }
      },
      {
        "id": 14,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "type": "logs",
        "title": "Recent Ollama Logs",
        "targets": [
          {
            "expr": "{container=\"ollama\"} |= \"error\" or |= \"warning\" or |= \"slow\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": true,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 34},
        "type": "table",
        "title": "Model Performance Summary",
        "targets": [
          {
            "expr": "avg by (model) (rate(ollama_request_duration_seconds_sum{model=~\"$model\"}[5m]) / rate(ollama_request_duration_seconds_count{model=~\"$model\"}[5m]))",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "sum by (model) (rate(ollama_request_total{model=~\"$model\"}[5m]))",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "avg by (model) (ollama_model_memory_bytes{model=~\"$model\"}/1024/1024)",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {"reducers": ["last"]}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {
                "Value #A": "Avg Response Time (s)",
                "Value #B": "Request Rate (req/s)", 
                "Value #C": "Memory Usage (MB)"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Avg Response Time (s)"},
              "properties": [
                {"id": "unit", "value": "s"},
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 5},
                      {"color": "red", "value": 10}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Memory Usage (MB)"},
              "properties": [
                {"id": "unit", "value": "decmbytes"}
              ]
            }
          ]
        }
      }
    ]
  }
}