{
  "dashboard": {
    "id": null,
    "uid": "ollama-performance",
    "title": "ULTRA Ollama Performance Dashboard",
    "tags": ["ollama", "ai", "performance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Response Time (seconds)",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(ollama_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ollama_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ollama_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "C"
          }
        ],
        "thresholds": [
          {"value": 10, "color": "yellow"},
          {"value": 30, "color": "red"}
        ],
        "alert": {
          "name": "High Ollama Response Time",
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [10]},
              "operator": {"type": "and"},
              "query": {"model": "A", "params": ["A", "5m", "now"]},
              "reducer": {"type": "avg", "params": []},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "message": "Ollama response time exceeded 10 seconds",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "stat",
        "title": "Current Response Time",
        "targets": [
          {
            "expr": "rate(ollama_request_duration_seconds_sum[1m]) / rate(ollama_request_duration_seconds_count[1m])",
            "legendFormat": "Avg Response Time",
            "instant": true,
            "refId": "A"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 5},
            {"color": "red", "value": 10}
          ]
        },
        "unit": "s"
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "graph",
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(ollama_request_total[1m])",
            "legendFormat": "Requests/min",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "graph",
        "title": "Success Rate %",
        "targets": [
          {
            "expr": "(rate(ollama_request_success_total[5m]) / rate(ollama_request_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "thresholds": [
          {"value": 95, "color": "green"},
          {"value": 90, "color": "yellow"},
          {"value": 80, "color": "red"}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{container_label_com_docker_compose_service=\"ollama\"}",
            "legendFormat": "Memory Usage",
            "refId": "A"
          },
          {
            "expr": "container_spec_memory_limit_bytes{container_label_com_docker_compose_service=\"ollama\"}",
            "legendFormat": "Memory Limit",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "bytes", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Concurrent Requests",
        "targets": [
          {
            "expr": "ollama_concurrent_requests",
            "legendFormat": "Active Requests",
            "refId": "A"
          },
          {
            "expr": "ollama_queued_requests",
            "legendFormat": "Queued Requests",
            "refId": "B"
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Model Load Time",
        "targets": [
          {
            "expr": "ollama_model_load_duration_seconds",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"show": false}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "type": "stat",
        "title": "Loaded Models",
        "targets": [
          {
            "expr": "ollama_loaded_models",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "type": "stat",
        "title": "Total Requests",
        "targets": [
          {
            "expr": "ollama_request_total",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "type": "stat",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(ollama_request_errors_total[5m])",
            "instant": true,
            "refId": "A"
          }
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "green", "value": null},
            {"color": "yellow", "value": 0.01},
            {"color": "red", "value": 0.05}
          ]
        },
        "unit": "percent"
      },
      {
        "id": 11,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "type": "stat",
        "title": "Model Status",
        "targets": [
          {
            "expr": "ollama_model_loaded{model=\"tinyllama\"}",
            "instant": true,
            "refId": "A"
          }
        ],
        "mappings": [
          {"type": "value", "value": "1", "text": "LOADED"},
          {"type": "value", "value": "0", "text": "NOT LOADED"}
        ],
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {"color": "red", "value": null},
            {"color": "green", "value": 1}
          ]
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "type": "table",
        "title": "Request Details",
        "targets": [
          {
            "expr": "topk(10, ollama_request_duration_seconds_sum) by (endpoint, model, status)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      }
    ]
  }
}