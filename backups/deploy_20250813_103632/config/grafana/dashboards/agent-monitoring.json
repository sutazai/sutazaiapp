{
  "dashboard": {
    "id": null,
    "uid": "agent-monitoring",
    "title": "Agent Performance Dashboard",
    "tags": ["agents", "monitoring", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate by Agent",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(agent_request_total[5m])",
            "legendFormat": "{{agent}} - {{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "show": true},
          {"format": "short", "show": false}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Error Rate by Agent",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(agent_error_total[5m])",
            "legendFormat": "{{agent}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "show": true},
          {"format": "short", "show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.05],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "name": "High Error Rate",
          "message": "Error rate exceeds 5%"
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Queue Latency (p95)",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_queue_latency_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"format": "short", "show": false}
        ],
        "thresholds": [
          {
            "value": 0.3,
            "colorMode": "critical",
            "fill": true,
            "line": true,
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "DB Query Duration (p95)",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - {{query_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"format": "short", "show": false}
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "type": "singlestat",
        "title": "Active Requests",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(agent_active_requests)",
            "refId": "A"
          }
        ],
        "format": "none",
        "sparkline": {
          "show": true,
          "lineColor": "rgb(31, 120, 193)"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "type": "singlestat",
        "title": "Agent Health Status",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(agent_health_status)",
            "refId": "A"
          }
        ],
        "format": "none",
        "thresholds": "0,1",
        "colors": ["red", "yellow", "green"],
        "colorBackground": true
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "type": "table",
        "title": "Agent Status Overview",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "agent_health_status",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "columns": [
          {"text": "Agent", "value": "agent"},
          {"text": "Status", "value": "Value"}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "type": "graph",
        "title": "Processing Duration by Agent",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(agent_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(agent_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(agent_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "show": true},
          {"format": "short", "show": false}
        ]
      }
    ]
  }
}