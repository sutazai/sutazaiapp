{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Cleanup Integration Report",
  "type": "object",
  "required": ["ts", "phase", "task_id", "suite", "passed", "failed", "duration_s", "artifacts"],
  "properties": {
    "ts": {"type": "string", "format": "date-time"},
    "phase": {"type": "string", "enum": ["discovery", "resolution", "integration", "validation"]},
    "task_id": {"type": "string"},
    "suite": {"type": "string"},
    "passed": {"type": "integer", "minimum": 0},
    "failed": {"type": "integer", "minimum": 0},
    "duration_s": {"type": "number", "minimum": 0},
    "artifacts": {
      "type": "array",
      "items": {"type": "string"}
    },
    "notes": {"type": "string"}
  }
}

