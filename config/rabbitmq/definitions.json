  "policies": [
    {
      "vhost": "/sutazai",
      "name": "ha-policy",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic",
        "message-ttl": 3600000,
        "max-length": 10000
      },
      "priority": 1
    },
    {
      "vhost": "/agents",
      "name": "agent-policy",
      "pattern": "agent\\..*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "exactly",
        "ha-params": 2,
        "ha-sync-mode": "automatic",
        "message-ttl": 1800000,
        "max-length": 5000,
        "overflow": "reject-publish"
      },
      "priority": 2
    },
    {
      "vhost": "/monitoring",
      "name": "monitoring-policy",
      "pattern": "metrics\\..*",
      "apply-to": "queues",
      "definition": {
        "ha-mode": "all",
        "message-ttl": 300000,
        "max-length": 50000,
        "overflow": "drop-head"
      },
      "priority": 1
    }
  ],
