{
  "services": [
    {
      "id": "backend-api",
      "name": "backend-api",
      "tags": ["api", "backend", "fastapi"],
      "address": "sutazai-backend",
      "port": 8000,
      "check": {
        "id": "backend-health",
        "name": "Backend Health Check",
        "http": "http://sutazai-backend:8000/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "frontend-ui",
      "name": "frontend-ui",
      "tags": ["ui", "frontend", "streamlit"],
      "address": "sutazai-frontend",
      "port": 8501,
      "check": {
        "id": "frontend-health",
        "name": "Frontend Health Check",
        "http": "http://sutazai-frontend:8501/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "ai-orchestrator",
      "name": "ai-orchestrator",
      "tags": ["agent", "orchestrator"],
      "address": "sutazai-ai-agent-orchestrator",
      "port": 8080,
      "check": {
        "id": "orchestrator-health",
        "name": "AI Orchestrator Health",
        "http": "http://sutazai-ai-agent-orchestrator:8080/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "task-coordinator",
      "name": "task-coordinator",
      "tags": ["agent", "coordinator"],
      "address": "sutazai-task-assignment-coordinator",
      "port": 8080,
      "check": {
        "id": "coordinator-health",
        "name": "Task Coordinator Health",
        "http": "http://sutazai-task-assignment-coordinator:8080/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "resource-arbitrator",
      "name": "resource-arbitrator",
      "tags": ["agent", "arbitrator"],
      "address": "sutazai-resource-arbitration-agent",
      "port": 8080,
      "check": {
        "id": "arbitrator-health",
        "name": "Resource Arbitrator Health",
        "http": "http://sutazai-resource-arbitration-agent:8080/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "hardware-optimizer",
      "name": "hardware-optimizer",
      "tags": ["agent", "hardware"],
      "address": "sutazai-hardware-resource-optimizer",
      "port": 8080,
      "check": {
        "id": "hardware-health",
        "name": "Hardware Optimizer Health",
        "http": "http://sutazai-hardware-resource-optimizer:8080/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "postgresql-db",
      "name": "postgresql",
      "tags": ["database", "postgres"],
      "address": "sutazai-postgres",
      "port": 5432,
      "check": {
        "id": "postgres-tcp",
        "name": "PostgreSQL TCP Check",
        "tcp": "sutazai-postgres:5432",
        "interval": "30s",
        "timeout": "5s"
      }
    },
    {
      "id": "redis-cache",
      "name": "redis",
      "tags": ["cache", "redis"],
      "address": "sutazai-redis",
      "port": 6379,
      "check": {
        "id": "redis-tcp",
        "name": "Redis TCP Check",
        "tcp": "sutazai-redis:6379",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "qdrant-vectordb",
      "name": "qdrant",
      "tags": ["vectordb", "qdrant"],
      "address": "sutazai-qdrant",
      "port": 6333,
      "check": {
        "id": "qdrant-health",
        "name": "Qdrant Health Check",
        "http": "http://sutazai-qdrant:6333/health",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "rabbitmq-broker",
      "name": "rabbitmq",
      "tags": ["messaging", "amqp"],
      "address": "sutazaiapp-rabbitmq",
      "port": 5672,
      "check": {
        "id": "rabbitmq-tcp",
        "name": "RabbitMQ TCP Check",
        "tcp": "sutazaiapp-rabbitmq:5672",
        "interval": "10s",
        "timeout": "3s"
      }
    },
    {
      "id": "ollama-llm",
      "name": "ollama",
      "tags": ["llm", "tinyllama"],
      "address": "sutazai-ollama",
      "port": 11434,
      "check": {
        "id": "ollama-health",
        "name": "Ollama API Check",
        "http": "http://sutazai-ollama:11434/api/tags",
        "interval": "30s",
        "timeout": "5s"
      }
