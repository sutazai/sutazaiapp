{
  "services": [
    {
      "id": "ollama-1",
      "name": "ollama",
      "tags": ["ai", "llm", "inference"],
      "address": "ollama",
      "port": 11434,
      "meta": {
        "version": "latest",
        "models": "tinyllama,deepseek-r1:8b,qwen3:8b,codellama:7b,llama2"
      },
      "check": {
        "http": "http://ollama:11434/api/tags",
        "interval": "30s",
        "timeout": "10s"
      },
      "weights": {
        "passing": 10,
        "warning": 1
      }
    },
    {
      "id": "chromadb-1",
      "name": "chromadb",
      "tags": ["ai", "vectordb", "storage"],
      "address": "chromadb",
      "port": 8000,
      "meta": {
        "version": "latest",
        "type": "vector-database"
      },
      "check": {
        "http": "http://chromadb:8000/api/v1/heartbeat",
        "interval": "30s",
        "timeout": "10s"
      }
    },
    {
      "id": "qdrant-1",
      "name": "qdrant",
      "tags": ["ai", "vectordb", "storage"],
      "address": "qdrant",
      "port": 6333,
      "meta": {
        "version": "latest",
        "type": "vector-database"
      },
      "check": {
        "http": "http://qdrant:6333/health",
        "interval": "30s",
        "timeout": "10s"
      }
    },
    {
      "id": "redis-1",
      "name": "redis",
      "tags": ["cache", "storage", "pubsub"],
      "address": "redis",
      "port": 6379,
      "meta": {
        "version": "7",
        "type": "cache"
      },
      "check": {
        "tcp": "redis:6379",
        "interval": "10s",
        "timeout": "5s"
      }
    },
    {
      "id": "rabbitmq-1",
      "name": "rabbitmq",
      "tags": ["queue", "messaging", "amqp"],
      "address": "rabbitmq",
      "port": 5672,
      "meta": {
        "version": "3.12",
        "type": "message-queue"
      },
      "check": {
        "tcp": "rabbitmq:5672",
        "interval": "10s",
        "timeout": "5s"
      }
    }
  ]
}