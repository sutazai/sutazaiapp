        "dataFormat": "timeseries",
        "heatmap": {},
        "highlightCards": true,
        "hideZeroBuckets": false
      },
      {
        "id": 3,
        "title": "Response Time by Endpoint (p50, p90, p99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(jarvis_latency_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p50 {{endpoint}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, sum(rate(jarvis_latency_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p90 {{endpoint}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(jarvis_latency_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "p99 {{endpoint}}",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 24,
          "x": 0,
          "y": 18
        },
        "xAxis": {
          "buckets": null,
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
