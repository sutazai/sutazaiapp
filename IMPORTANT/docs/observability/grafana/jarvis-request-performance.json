        "title": "Request Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(sum(rate(jarvis_requests_total{status_code!~\"4..|5..\"}[5m])) / sum(rate(jarvis_requests_total[5m]))) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 27
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "pluginVersion": "8.0.0",
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
