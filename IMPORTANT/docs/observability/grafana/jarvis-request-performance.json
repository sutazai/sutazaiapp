    "tags": ["jarvis", "performance", "requests"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate by Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(jarvis_requests_total[5m])) by (endpoint, method)",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "xAxis": {
          "buckets": null,
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
        },
        "yAxes": [
          {
            "label": "Requests per second",
            "logBase": 1,
            "max": null,
            "min": null,
            "show": true
          },
          {
            "label": null,
            "logBase": 1,
            "max": null,
            "min": null,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 1,
        "points": false,
        "pointradius": 5,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "avg": false,
          "current": false,
          "max": false,
          "min": false,
          "show": true,
          "total": false,
          "values": false
