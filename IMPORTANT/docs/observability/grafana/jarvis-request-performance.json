        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "aliasColors": {},
        "seriesOverrides": []
      },
      {
        "id": 2,
        "title": "Latency Distribution Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(jarvis_latency_seconds_bucket[5m])) by (le)",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 24,
          "x": 0,
          "y": 9
        },
        "xAxis": {
          "show": true
        },
        "yAxis": {
          "show": true,
          "label": "Latency (seconds)",
          "logBase": 1,
          "max": null,
          "min": null
        },
        "cards": {
          "cardPadding": null,
          "cardRound": null
        },
        "color": {
          "mode": "spectrum",
          "cardColor": "#b4ff00",
          "colorScale": "sqrt",
          "exponent": 0.5,
          "colorScheme": "interpolateOranges"
        },
        "legend": {
          "show": false
