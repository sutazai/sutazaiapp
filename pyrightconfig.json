{
    "include": [
        "backend"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        ".git",
        ".venv",
        "venv"
    ],
    "ignore": [
        "backend/services/doc_processing.py"
    ],
    "reportMissingImports": "warning",
    "reportMissingModuleSource": "warning",
    "pythonVersion": "3.11",
    "typeCheckingMode": "basic",
    "extraPaths": ["types"]
} 