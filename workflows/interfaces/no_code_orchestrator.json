{
  "interface_config": {
    "name": "SutazAI No-Code Agent Orchestrator",
    "version": "1.0",
    "description": "Visual, drag-and-drop interface for orchestrating AI agents without coding",
    "theme": "dark",
    "layout": "grid"
  },
  "components": {
    "toolbox": {
      "agent_nodes": [
        {
          "id": " system-architect",
          "display_name": "System Architect",
          "icon": "architecture",
          "color": "#FF6B35",
          "description": "Design and optimize system architecture",
          "inputs": ["requirements", "constraints"],
          "outputs": ["architecture_plan", "recommendations"]
        },
        {
          "id": "product-manager",
          "display_name": "Product Manager",
          "icon": "management",
          "color": "#004E89",
          "description": "Coordinate projects and find solutions",
          "inputs": ["project_scope", "requirements"],
          "outputs": ["project_plan", "resource_allocation"]
        },
        {
          "id": "autonomous-system-controller",
          "display_name": "System Controller",
          "icon": "control",
          "color": "#9A031E",
          "description": "Master orchestrator for autonomous operations",
          "inputs": ["system_state", "objectives"],
          "outputs": ["decisions", "actions"]
        },
        {
          "id": "senior-ai-engineer",
          "display_name": "AI Engineer",
          "icon": "ai",
          "color": "#5F0F40",
          "description": "AI/ML development and optimization",
          "inputs": ["model_requirements", "data"],
          "outputs": ["models", "optimizations"]
        },
        {
          "id": "infrastructure-devops-manager",
          "display_name": "DevOps Manager",
          "icon": "infrastructure",
          "color": "#FB8500",
          "description": "Infrastructure and deployment management",
          "inputs": ["deployment_config", "resources"],
          "outputs": ["deployment_status", "monitoring"]
        }
      ],
      "processing_nodes": [
        {
          "id": "data_transformer",
          "display_name": "Data Transformer",
          "icon": "transform",
          "color": "#219EBC",
          "description": "Transform data between formats",
          "inputs": ["input_data", "transformation_rules"],
          "outputs": ["transformed_data"]
        },
        {
          "id": "condition_checker",
          "display_name": "Condition Checker",
          "icon": "condition",
          "color": "#8ECAE6",
          "description": "Check conditions and route workflow",
          "inputs": ["data", "conditions"],
          "outputs": ["true_path", "false_path"]
        },
        {
          "id": "aggregator",
          "display_name": "Result Aggregator",
          "icon": "aggregate",
          "color": "#FFB3BA",
          "description": "Combine results from multiple sources",
          "inputs": ["results_array"],
          "outputs": ["aggregated_result"]
        }
      ],
      "trigger_nodes": [
        {
          "id": "manual_trigger",
          "display_name": "Manual Trigger",
          "icon": "play",
          "color": "#2E8B57",
          "description": "Start workflow manually",
          "outputs": ["trigger_data"]
        },
        {
          "id": "webhook_trigger",
          "display_name": "Webhook",
          "icon": "webhook",
          "color": "#32CD32",
          "description": "Start workflow via HTTP webhook",
          "outputs": ["webhook_data"]
        },
        {
          "id": "schedule_trigger",
          "display_name": "Scheduler",
          "icon": "clock",
          "color": "#98FB98",
          "description": "Start workflow on schedule",
          "config": ["cron_expression"],
          "outputs": ["scheduled_data"]
        }
      ]
    },
    "canvas": {
      "grid_size": 20,
      "snap_to_grid": true,
      "auto_layout": true,
      "zoom_levels": [0.25, 0.5, 0.75, 1.0, 1.25, 1.5, 2.0],
      "default_zoom": 1.0
    },
    "property_panel": {
      "sections": [
        {
          "name": "Basic Properties",
          "fields": ["name", "description", "enabled"]
        },
        {
          "name": "Configuration",
          "fields": ["timeout", "retry_attempts", "priority"]
        },
        {
          "name": "Monitoring",
          "fields": ["logging_enabled", "metrics_enabled", "alerts_enabled"]
        }
      ]
    }
  },
  "workflows": {
    "templates": [
      {
        "id": "simple_chat_workflow",
        "name": "Simple Chat Workflow",
        "description": "Basic conversational workflow with single agent",
        "complexity": "beginner",
        "nodes": [
          {
            "id": "trigger",
            "type": "manual_trigger",
            "position": {"x": 100, "y": 100}
          },
          {
            "id": "chat_agent",
            "type": "ai-product-manager",
            "position": {"x": 300, "y": 100},
            "config": {
              "temperature": 0.7,
              "max_tokens": 500
            }
          }
        ],
        "connections": [
          {"from": "trigger", "to": "chat_agent"}
        ]
      },
      {
        "id": "code_review_workflow",
        "name": "Automated Code Review",
        "description": "Multi-agent code review and improvement workflow",
        "complexity": "intermediate",
        "nodes": [
          {
            "id": "code_input",
            "type": "manual_trigger",
            "position": {"x": 50, "y": 150}
          },
          {
            "id": "code_analyzer",
            "type": "code-generation-improver",
            "position": {"x": 250, "y": 100}
          },
          {
            "id": "security_checker",
            "type": "security-pentesting-specialist",
            "position": {"x": 250, "y": 200}
          },
          {
            "id": "qa_validator",
            "type": "testing-qa-validator",
            "position": {"x": 450, "y": 150}
          },
          {
            "id": "result_merger",
            "type": "aggregator",
            "position": {"x": 650, "y": 150}
          }
        ],
        "connections": [
          {"from": "code_input", "to": "code_analyzer"},
          {"from": "code_input", "to": "security_checker"},
          {"from": "code_analyzer", "to": "qa_validator"},
          {"from": "security_checker", "to": "qa_validator"},
          {"from": "qa_validator", "to": "result_merger"}
        ]
      },
      {
        "id": "system_deployment_workflow",
        "name": "System Deployment Pipeline",
        "description": "Complete deployment workflow with multiple validation stages",
        "complexity": "advanced",
        "nodes": [
          {
            "id": "deploy_trigger",
            "type": "webhook_trigger",
            "position": {"x": 50, "y": 200}
          },
          {
            "id": "architect_review",
            "type": " -system-architect",
            "position": {"x": 200, "y": 100}
          },
          {
            "id": "devops_preparation",
            "type": "infrastructure-devops-manager",
            "position": {"x": 200, "y": 200}
          },
          {
            "id": "deployment_master",
            "type": "deployment-automation-master",
            "position": {"x": 200, "y": 300}
          },
          {
            "id": "system_validation",
            "type": "testing-qa-validator",
            "position": {"x": 400, "y": 200}
          },
          {
            "id": "performance_check",
            "type": "hardware-resource-optimizer",
            "position": {"x": 600, "y": 200}
          }
        ],
        "connections": [
          {"from": "deploy_trigger", "to": "architect_review"},
          {"from": "deploy_trigger", "to": "devops_preparation"},
          {"from": "deploy_trigger", "to": "deployment_master"},
          {"from": "architect_review", "to": "system_validation"},
          {"from": "devops_preparation", "to": "system_validation"},
          {"from": "deployment_master", "to": "system_validation"},
          {"from": "system_validation", "to": "performance_check"}
        ]
      }
    ]
  },
  "ui_features": {
    "drag_and_drop": true,
    "real_time_validation": true,
    "workflow_preview": true,
    "execution_monitoring": true,
    "version_control": true,
    "collaboration": true,
    "export_formats": ["json", "yaml", "python"],
    "import_formats": ["json", "yaml"]
  },
  "user_roles": {
    "admin": {
      "permissions": ["create", "edit", "delete", "execute", "manage_users"],
      "access_level": "full"
    },
    "developer": {
      "permissions": ["create", "edit", "execute", "view"],
      "access_level": "standard"
    },
    "operator": {
      "permissions": ["execute", "view"],
      "access_level": "limited"
    }
  }
}