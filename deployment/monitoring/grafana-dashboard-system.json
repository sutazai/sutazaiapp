{
  "dashboard": {
    "id": null,
    "title": "SutazAI System Overview",
    "tags": ["sutazai", "system", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"sutazai-backend\"}",
            "legendFormat": "Backend",
            "refId": "A"
          },
          {
            "expr": "up{job=\"ollama\"}",
            "legendFormat": "Ollama",
            "refId": "B"
          },
          {
            "expr": "up{job=\"postgres\"}",
            "legendFormat": "Database",
            "refId": "C"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active AI Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "count(up{job=\"sutazai-agents\"} == 1)",
            "legendFormat": "Active Agents",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "CPU Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"sutazai-.*\"}[5m]) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "timeseries", 
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\"sutazai-.*\"} / 1024 / 1024 / 1024",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "GB"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "API Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"sutazai-backend\"}[5m])",
            "legendFormat": "{{method}} {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17}
      },
      {
        "id": 6,
        "title": "API Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"sutazai-backend\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{service=\"sutazai-backend\"}[5m]))",
            "legendFormat": "Median",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17}
      },
      {
        "id": 7,
        "title": "Agent Task Processing",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(agent_tasks_completed_total[5m])",
            "legendFormat": "{{agent_name}} - Completed",
            "refId": "A"
          },
          {
            "expr": "rate(agent_tasks_failed_total[5m])",
            "legendFormat": "{{agent_name}} - Failed", 
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "tps"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 26}
      },
      {
        "id": 8,
        "title": "Ollama Model Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ollama_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile latency",
            "refId": "A"
          },
          {
            "expr": "ollama_queue_size",
            "legendFormat": "Queue size",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 26}
      },
      {
        "id": 9,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "pg_stat_database_tup_returned{datname=\"sutazai\"}",
            "legendFormat": "Rows returned",
            "refId": "A"
          },
          {
            "expr": "pg_stat_database_tup_inserted{datname=\"sutazai\"}",
            "legendFormat": "Rows inserted",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 35}
      },
      {
        "id": 10,
        "title": "Network I/O",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_network_transmit_bytes_total{name=~\"sutazai-.*\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} - TX",
            "refId": "A"
          },
          {
            "expr": "rate(container_network_receive_bytes_total{name=~\"sutazai-.*\"}[5m]) / 1024 / 1024",
            "legendFormat": "{{name}} - RX",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "MBps"
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 35}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 0
  }
}